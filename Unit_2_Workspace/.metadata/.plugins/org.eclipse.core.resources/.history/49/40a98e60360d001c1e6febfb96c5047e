#include <stdio.h>
void main(){
	int number, factorial = 0 ;
	printf("Enter a number: \n");
	fflush(stdin); fflush(stdout);
	scanf("%d", &number);

	for(int i=1; i<=number; ++i){
		factorial *= i;
	}
	printf("Factorial = %d\n", factorial);
}
